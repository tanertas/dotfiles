    <!-- Visual Styles -->
    <WindowStyle>
        <Font>Ubuntu Condensed-9:bold</Font>
        <Width>2</Width>
        <Height>20</Height>
        <Corner>0</Corner>
        <Foreground>#FFFFFF</Foreground>
        <Background>#555555</Background>
        <!-- <Opacity>0.9:0.9:0.1</Opacity> -->
        <Opacity>1.0</Opacity>
        <Active>
            <Foreground>#FFFFFF</Foreground>
            <Background>#0077CC</Background>
            <Opacity>1.0</Opacity>
        </Active>
    </WindowStyle>

    <TrayStyle group="false" list="all">
        <!-- <Font>DejaVu Sans-8:condensed</Font> -->
        <Font>Hack Nerd Font-14</Font>
        <Background>#000000</Background>
        <Foreground>#FFFFFF</Foreground>
        <Opacity>0.75</Opacity>
    </TrayStyle>

    <TaskListStyle>
        <Font>DejaVu Sans-8:condensed</Font>
        <Outline>#000000</Outline>
        <Text>#FFFFFF</Text>
    </TaskListStyle>

    <PagerStyle>
        <Font>Ubuntu Condensed-9:bold</Font>
        <Outline>#000000</Outline>
        <Foreground>#555555</Foreground>
        <Background>#333333</Background>
        <Text>#FFFFFF</Text>
        <Active>
            <Foreground>#0077CC</Foreground>
            <Background>#555555</Background>
        </Active>
    </PagerStyle>

    <MenuStyle>
        <Font>Hack Nerd Font-16</Font>
        <Foreground>#FFFFFF</Foreground>
        <Background>#333333</Background>
        <Outline>#000000</Outline>
        <Active>
            <Foreground>#FFFFFF</Foreground>
            <Background>#0077CC</Background>
        </Active>
        <Opacity>0.75</Opacity>
    </MenuStyle>

    <PopupStyle>
        <Font>DejaVu Sans-8:condensed</Font>
        <Foreground>#000000</Foreground>
        <Background>#999999</Background>
    </PopupStyle>

    <ClockStyle>
        <Font>DejaVu Sans-8:condensed</Font>
        <Outline>#000000</Outline>
        <Text>#FFFFFF</Text>
    </ClockStyle>

    <!-- Path where icons can be found.
         IconPath can be listed multiple times to allow searching
         for icons in multiple paths.
      -->
    <IconPath>
        $HOME/.icons/jwm/
    </IconPath>
    <IconPath>
	/usr/share/pixmaps
    </IconPath>

    <!-- Virtual Desktops -->
    <!-- Desktop tags can be contained within Desktops for desktop names. -->
    <Desktops width="2" height="1">
        <!-- Default background. Note that a Background tag can be
              contained within a Desktop tag to give a specific background
              for that desktop.
         -->
        <!-- <Background type="solid">#111111</Background> -->
    </Desktops>

    <!-- Double click speed (in milliseconds) -->
    <DoubleClickSpeed>300</DoubleClickSpeed>

    <!-- Double click delta (in pixels) -->
    <DoubleClickDelta>2</DoubleClickDelta>

    <!-- The focus model (sloppy or click) -->
    <FocusModel>click</FocusModel>

    <!-- The snap mode (none, screen, or border) -->
    <SnapMode distance="10">border</SnapMode>

    <!-- The move mode (outline or opaque) -->
    <MoveMode>opaque</MoveMode>

    <!-- The resize mode (outline or opaque) -->
    <ResizeMode>outline</ResizeMode>

    <!-- Key bindings -->
    <Key key="Up">up</Key>
    <Key key="Down">down</Key>
    <Key key="Right">right</Key>
    <Key key="Left">left</Key>
    <Key key="h">left</Key>
    <Key key="j">down</Key>
    <Key key="k">up</Key>
    <Key key="l">right</Key>
    <Key key="Return">select</Key>
    <Key key="Escape">escape</Key>
<!--    <Key key="Print">exec:maim ~/dwhelper/.z/maim/$(date +%F-%T).png</Key> -->
    <Key key="Print">exec:scrot ~/images/screenshots/'%Y-%m-%d-%H%M%S_$wx$h_scrot.png' -q 90</Key>
    <Key key="XF86Display">exec:toggle-vga.sh</Key>
    <Key key="XF86Calculator">exec:galculator</Key>
    <Key key="XF86Tools">exec:urxvtc</Key>
    <Key mask="4" key="F3">exec:toggle-vga.sh</Key>
    <Key key="XF86AudioMute">exec:amixer -c CA0106 -q sset Master toggle</Key>
    <Key key="XF86AudioRaiseVolume">exec:amixer -c CA0106 -q sset Master 3%+</Key>
    <Key key="XF86AudioLowerVolume">exec:amixer -c CA0106 -q sset Master 3%-</Key>
    <Key mask="A" key="Tab">nextstacked</Key>
    <Key mask="A" key="F4">close</Key>
    <Key mask="A" key="#">desktop#</Key>
    <Key mask="A" key="F1">root:1</Key>
<!--    <Key mask="A" key="F2">window</Key> -->
    <Key mask="A" key="F2">exec: dmenu_run -f</Key>
    <Key mask="A" key="F10">maximize</Key>
    <!-- <Key mask="A" key="Right">rdesktop</Key>
    <Key mask="A" key="Left">ldesktop</Key>
    <Key mask="A" key="Up">udesktop</Key>
    <Key mask="A" key="Down">ddesktop</Key> -->
<StartupCommand>~/bin/startup.sh</StartupCommand>

    <!-- Options for program groups. -->
    <Group>
        <Class>xarchiver</Class>
        <Class>Xarchiver</Class>
        <Option>layer:above</Option>
    </Group>
    <Group>
        <Class>Pidgin</Class>
        <Option>sticky</Option>
    </Group>
    <Group>
        <Name>xterm</Name>
        <Option>vmax</Option>
    </Group>
    <Group>
        <Name>xclock</Name>
        <Option>drag</Option>
        <Option>notitle</Option>
    </Group>
